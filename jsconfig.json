{
  "allowJs": true,
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@settings": ["settings.js"],
      "@utils/*": ["src/utils/*"],
      "@test-utils/*": ["src/test-utils/*"],
      "@components/*": ["src/components/*"],
      "@slices/*": ["src/slices/*"],
      "@actions/*": ["src/actions/*"],
      "@hooks/*": ["src/hooks/*"]
    }
  },
  "exclude": ["node_modules"]
}
